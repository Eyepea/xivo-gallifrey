# $Revision$
# $Date$

include $(NONO_MK)/autobuild.mk

DEB_PKG = "asterisk-app-conference"

SRCVER = $(shell cat $(MODULE_PATH)/SOURCE-VERSION)
TARBALL_NAME = "asterisk-app-conference_$(SRCVER).orig.tar.gz"
UPSTREAM_VER = $(SRCVER)+pf.xivo.$(MAINREV)~svn$(SVNREV)


default:

do-tarball::
	cp $(MODULE_PATH)/tarballs/$(TARBALL_NAME) $(DEB_DESTDIR)/$(DEB_PKG)_$(UPSTREAM_VER).orig.tar.gz

