DIST_NAME="Xivo"
DIST_VERSION="0.3"
DIST_FS="ext2fs"
DIST_CONSOLE="ttyS0"
DIST_CONSOLE_SPEED="115200"
ARCH="i386"
TARGET="i486-linux-gnu"
SYSBOOTIMGSIZE="8"
SYSROOTIMGSIZE="32"
SYSCONFIMGSIZE="64"

gen_linux_conf()
{
  gen_linux_conf_xivo
}

gen_fstab()
{
  gen_fstab_xivo
}

gen_modules()
{
  gen_modules_xivo
}

make_dist_packages()
{
  make_dist_packages_xivo
}
