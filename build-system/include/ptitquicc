gen_fstab_ptitquicc()
{
  cat << EOF
proc           /proc proc  rw 0 0
/dev/mtdblock3 /mnt  jffs2 rw 0 0
EOF
}

gen_modules_ptitquicc()
{
  cat << EOF
EOF
}

make_dist_packages_ptitquicc()
{
  make_dropbear
  make_vsftpd
  make_asterisk
  make_chan_ooh323
  make_pwlib
}
