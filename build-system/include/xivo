gen_fstab_xivo()
{
  cat << EOF
proc      /proc proc rw             0 0
/dev/hdc1 /boot ext2 rw,sync,noauto 1 2
/dev/hdc4 /mnt  ext2 rw,sync        1 2
EOF
}

# Developer note :
# vISDN modules :
# lapd
# visdn-netdev
# visdn-streamport
# visdn-timer-system
# visdn-ec
# visdn-hfc-pci
# 
# mISDN modules :
# hfcpci protocol=0x2,0x12 layermask=0xf,0x3
# mISDN_dsp
gen_modules_xivo()
{
  cat << EOF
e100
8021q
ztdummy
wctdm
hfcpci protocol=0x2,0x12 layermask=0xf,0x3
mISDN_core
mISDN_l1
mISDN_l2
l3udss1
mISDN_dsp
EOF
}

make_dist_packages_xivo()
{
  make_iptables
  make_dropbear
  make_vsftpd
  make_lighttpd
  make_php
  make_php_fileinfo
  make_php_sqlite
  make_zaptel
  make_asterisk
  make_res_sqlite
  make_chan_misdn
}
