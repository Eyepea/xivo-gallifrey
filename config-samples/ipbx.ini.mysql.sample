section.sip = true
section.iax = true
section.group = true
section.queue = true
section.voicemail = true
section.meetme = true

[ami]
bindaddr = 192.168.0.77
port = 5004

[autoprov]
host = 127.0.0.1
port = 8666
datastorage.type = mysql
datastorage.user = asterisk
